{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "paths": {
      "@wsh-2025/client/*": ["./*"]
    },
    "skipLibCheck": true,
    "target": "ESNext",
    "verbatimModuleSyntax": true
  },
  "exclude": ["dist"],
  "extends": "@wsh-2025/configs/tsconfig.json",
  "include": ["**/*", "**/*.mjs"]
}
